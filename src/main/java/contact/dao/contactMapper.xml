<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="contactSQL">
	<!-- 글쓰기 -->
	<insert id="contactWrite" parameterType="contact.bean.ContactDTO">
	insert into contact values(seq_contact.nextval, #{name },#{email },#{phone },#{subject },#{content }, SYSDATE)
	</insert>

</mapper>

















